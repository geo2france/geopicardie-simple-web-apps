    <!--
    L'attribut style = "gallery" permet d'afficher les fonds de carte dans une liste avec incrustation de vignette pour chaque layer
    L'attribut style = "default" permet d'afficher la liste des couches de fond sous forme d'un groupe de boutons.
    -->
    <baselayers style="gallery"><!-- style="default"||gallery" -->
        <baselayer
                type="WMS"
                id="osm-geopic-bright-wms"
                label="OSM - GéoPicardie fond contrasté"
                title="OpenStreetMap - fond GéoPicardie contrasté"
                thumbgallery="apps/eolien/basemap/osm.geopicardie.bright.png"
                url="http://osm.geopicardie.fr/mapproxy/service"
                layers="bright"
                format="image/png"
                tiled="false"
                attribution="Données : les contributeurs d'&lt;a href='http://www.openstreetmap.org/' target='_blank'&gt;OpenStreetMap &lt;/a&gt;,  &lt;a href='http://www.openstreetmap.org/copyright' target='_blank'>ODbL &lt;/a&gt;"
                visible="true"/>
        <baselayer
                type="WMS"
                id="osm-geopic-faded-wms"
                label="OSM - GéoPicardie fond délavé"
                title="OpenStreetMap - fond GéoPicardie en demi-teintes"
                thumbgallery="apps/eolien/basemap/osm.geopicardie.faded.png"
                url="http://osm.geopicardie.fr/mapproxy/service"
                layers="faded"
                format="image/png"
                tiled="false"
                attribution="Données : les contributeurs d'&lt;a href='http://www.openstreetmap.org/' target='_blank'&gt;OpenStreetMap &lt;/a&gt;,  &lt;a href='http://www.openstreetmap.org/copyright' target='_blank'>ODbL &lt;/a&gt;"
                visible="false"/>
        <baselayer
                type="WMS"
                id="osm-geopic-grey-wms"
                label="OSM - GéoPicardie fond en niveaux de gris"
                title="OpenStreetMap - fond GéoPicardie en niveaux de gris"
                thumbgallery="apps/eolien/basemap/osm.geopicardie.grey.png"
                url="http://osm.geopicardie.fr/mapproxy/service"
                layers="grey"
                format="image/png"
                tiled="false"
                attribution="Données : les contributeurs d'&lt;a href='http://www.openstreetmap.org/' target='_blank'&gt;OpenStreetMap &lt;/a&gt;,  &lt;a href='http://www.openstreetmap.org/copyright' target='_blank'>ODbL &lt;/a&gt;"
                visible="false"/>
        <baselayer
                type="OSM"
                id="stamen2"
                label="Watercolor"
                title="Stamen Design"
                thumbgallery="img/basemap/watercolor.jpg"
                url="http://{a-c}.tile.stamen.com/watercolor/{z}/{x}/{y}.jpg"
                maxzoom="20"
                visible="false"
                attribution="Map tiles by &lt;a href='http://stamen.com/'&gt;Stamen Design &lt;/a&gt;, under &lt;a href='http://creativecommons.org/licenses/by/3.0/'&gt;CC BY 3.0 &lt;/a&gt;"/>
        <baselayer
                type="OSM"
                id="stamen1"
                label="Toner-lite"
                title="Stamen Design"
                thumbgallery="img/basemap/toner-lite.png"
                url="http://{a-d}.tile.stamen.com/toner-lite/{z}/{x}/{y}.png"
                maxzoom="20"
                visible="false"
                attribution="Map tiles by &lt;a href='http://stamen.com/'&gt;Stamen Design &lt;/a&gt;, under &lt;a href='http://creativecommons.org/licenses/by/3.0/'&gt;CC BY 3.0 &lt;/a&gt;"/>
        <baselayer
                type="OSM"
                id="darkmatter"
                label="Dark Matter"
                title="CartoDb"
                thumbgallery="img/basemap/darkmatter.png"
                url="http://{a-c}.basemaps.cartocdn.com/dark_all/{z}/{x}/{y}.png"
                maxzoom="20"
                visible="true"
